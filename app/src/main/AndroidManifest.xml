<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.sanpra.checklist"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk />

    <application android:icon="@drawable/launcher_icon" android:label="@string/app_name"
        android:name=".application.ChecklistApp"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
        >
        <activity android:name="org.sanpra.checklist.activity.ChecklistActivity"
                  android:label="@string/app_name"
                  android:theme="@style/AppTheme"
                  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>